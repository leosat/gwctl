ROOT=/gwctl
RUNTIME_CONFFILE_SUFFIX=runtime_conf
DMN_IN=$ROOT/var/dmn_in
WWW_IN=$ROOT/var/www_in
CURRENT_CHANF_PREFIX=$ROOT/var/cstatef
PENDING_CHANF_PREFIX=$ROOT/var/pstatef
DMNPIDF=$ROOT/var/gwctld.pid
DMNLOGF=$ROOT/var/gwctld.log
MODEF=$ROOT/var/mode
TSFILE=$ROOT/var/tsfile
GWCTLD=$ROOT/bin/gwctld
GWCTLD_RC_SCRIPT=$ROOT/etc/rc.gwctld
SWITCH_SCRIPT_NAME=${ROOT}/bin/switch.sh
SWITCH_SCRIPT=$SWITCH_SCRIPT_NAME
WWW_IFACE_HOSTNAME=${SERVER_NAME:-`hostname`}
WWW_HEADER=$ROOT/www/header
COMMON_SH_LIB=$ROOT/lib/common
GWCTLD_SEC_TO_SLEEP=15
SW_RES="switch"
LCK_FILE_PREFIX=$ROOT/var/lck

